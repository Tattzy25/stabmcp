# FastMCP Server Environment Configuration
# Copy this file to .env and update the values for your environment

# Server Configuration
PORT=8080
HOST=0.0.0.0
NODE_ENV=production

# MCP Server Configuration
MCP_SERVER_NAME=TaaTTTy
MCP_SERVER_VERSION=1.0.0

# Security Configuration (for production)
# CORS_ORIGIN=https://your-domain.com
# API_KEY=your-secure-api-key

# Monitoring Configuration
# HEALTH_CHECK_PATH=/health
# METRICS_PATH=/metrics

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=app.log

# Resource Limits
MAX_REQUEST_SIZE=10mb
REQUEST_TIMEOUT=30000

# External Services (if needed)
# DATABASE_URL=your-database-connection-string
# REDIS_URL=your-redis-connection-string

# Stability AI Configuration
STABILITY_API_KEY=your_stability_ai_api_key_here
STABILITY_ENGINE=stable-diffusion-xl-1024-v1-0
STABILITY_WIDTH=1024
STABILITY_HEIGHT=1024
STABILITY_STEPS=50
STABILITY_CFG_SCALE=7
STABILITY_SAMPLER=k_lms
SAVE_METADATA=TRUE
SAVE_METADATA_FAILED=FALSE
{
  "name": "stabmcp",
  "version": "1.0.0",
  "description": "Production-grade MCP server for Stability AI image endpoints",
  "main": "dist/server.js",
  "bin": {
    "stabmcp": "./dist/server.js"
  },
  "scripts": {
